@Article{Suto1989,
author="S{\"u}t{\H{o}}, Andr{\'a}s",
title="Singular continuous spectrum on a cantor set of zero Lebesgue measure for the Fibonacci Hamiltonian",
journal="Journal of Statistical Physics",
volume="56",
number="3",
pages="525--531",
abstract="It is rigorously proven that the spectrum of the tight-binding Fibonacci Hamiltonian,Hmn=$\delta$m, n+1+$\delta$m, n−1+$\delta$                  m, n                $\mu$[(n+1)$\alpha$]−[n$\alpha$]) where $\alpha$=({\textsurd}5−1)/2 and [{\textperiodcentered}] means integer part, is a Cantor set of zero Lebesgue measure for all real nonzero$\mu$, and the spectral measures are purely singular continuous. This follows from a recent result by Kotani, coupled with the vanishing of the Lyapunov exponent in the spectrum.",
issn="1572-9613",
doi="10.1007/BF01044450",
url="http://dx.doi.org/10.1007/BF01044450"
}

