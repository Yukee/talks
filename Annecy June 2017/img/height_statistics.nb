Cell[BoxData[{
 RowBox[{
  RowBox[{"bisec", "[", 
   RowBox[{"f_", ",", 
    RowBox[{"{", 
     RowBox[{"a_", ",", "b_"}], "}"}]}], "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"f", "[", "a", "]"}], " ", 
      RowBox[{"f", "[", "b", "]"}]}], "\[LessEqual]", "0"}], ",", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"f", "[", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"a", "+", "b"}], ")"}], "/", "2"}], "]"}], " ", 
        RowBox[{"f", "[", "a", "]"}]}], "\[LessEqual]", "0"}], ",", 
      RowBox[{"{", 
       RowBox[{"a", ",", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"a", "+", "b"}], ")"}], "/", "2"}]}], "}"}], ",", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"f", "[", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"a", "+", "b"}], ")"}], "/", "2"}], "]"}], " ", 
          RowBox[{"f", "[", "b", "]"}]}], "<", "0"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"a", "+", "b"}], ")"}], "/", "2"}], ",", "b"}], "}"}]}], 
       "]"}]}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"0", ",", "0"}], "}"}]}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"linie", "[", 
    RowBox[{"f_", ",", 
     RowBox[{"{", 
      RowBox[{"a_", ",", "b_"}], "}"}], ",", "1"}], "]"}], "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Min", "[", 
      RowBox[{"a", ",", "b"}], "]"}], ",", 
     RowBox[{"Max", "[", 
      RowBox[{"a", ",", "b"}], "]"}]}], "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"linie", "[", 
    RowBox[{"f_", ",", 
     RowBox[{"{", 
      RowBox[{"a_", ",", "b_"}], "}"}], ",", "n_"}], "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"f", "[", "a", "]"}], " ", 
       RowBox[{"f", "[", "b", "]"}]}], "\[LessEqual]", "0"}], ",", 
     RowBox[{"bisec", "[", 
      RowBox[{"f", ",", 
       RowBox[{"linie", "[", 
        RowBox[{"f", ",", 
         RowBox[{"{", 
          RowBox[{"a", ",", "b"}], "}"}], ",", 
         RowBox[{"n", "-", "1"}]}], "]"}]}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"0", ",", "0"}], "}"}]}], "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"BisectionMethod", "[", 
   RowBox[{"f_", ",", 
    RowBox[{"{", 
     RowBox[{"a_", ",", "b_"}], "}"}], ",", "tol_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"evalf", ",", 
      RowBox[{"an", "=", "a"}], ",", 
      RowBox[{"bn", "=", "b"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "store", " ", "values", " ", "of", " ", "f", " ", "to", " ", "speed", 
      " ", "up", " ", "computations"}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"evalf", "[", "x_", "]"}], ":=", 
      RowBox[{
       RowBox[{"evalf", "[", "x", "]"}], "=", 
       RowBox[{"f", "[", "x", "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "bisect", " ", "while", " ", "the", " ", "root", " ", "frame", " ", 
       "is", " ", "larger", " ", "than", " ", "tol"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"bn", "-", "an"}], "\[GreaterEqual]", "tol"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"an", ",", "bn"}], "}"}], "=", 
        RowBox[{"linie", "[", 
         RowBox[{"evalf", ",", 
          RowBox[{"{", 
           RowBox[{"an", ",", "bn"}], "}"}], ",", "2"}], "]"}]}]}], "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"an", ",", "bn"}], "}"}]}]}], "]"}]}]}], "Input",
 CellChangeTimes->{{3.6680004179170113`*^9, 3.668000576090518*^9}, {
