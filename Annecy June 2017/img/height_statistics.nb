Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "largest", " ", "eigenvalue", " ", "of", " ", "the", " ", "inflation", " ",
     "matrix", " ", 
    RowBox[{"T", "[", "z", "]"}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"v", "[", "z_", "]"}], ":=", 
   FractionBox[
    RowBox[{"4", "+", 
     RowBox[{"9", "z"}], "+", 
     RowBox[{"4", " ", 
      SuperscriptBox["z", "2"]}], "+", 
     RowBox[{"2", " ", 
