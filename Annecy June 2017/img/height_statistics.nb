Cell[CellGroupData[{

Cell["\<\
Moments of the distribution of heights, generalized inflation matrix, \
frequencies\
\>", "Subsection",
 CellChangeTimes->{{3.6679049639539213`*^9, 3.6679049694419003`*^9}, {
  3.667905111754107*^9, 3.667905151994074*^9}}],

Cell[TextData[{
 "We are interested in computing the moments of the distribution of heights:\n\
",
 Cell[BoxData[
  FormBox[
   RowBox[{"\n", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox[
        OverscriptBox["N", "~"], "\[Mu]"], "^", 
       RowBox[{"(", "l", ")"}]}], 
      RowBox[{"(", "t", ")"}]}], " ", "=", " ", 
     RowBox[{
      SubscriptBox["\[CapitalSigma]", "m"], 
      RowBox[{"t", "^", "m"}], " ", 
      RowBox[{
       SubscriptBox["N", "\[Mu]"], "^", 
       RowBox[{"(", "l", ")"}]}], 
      RowBox[{"(", "m", ")"}]}]}]}], TraditionalForm]]],
 "\nWe have:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["N", "\[Mu]"], "^", 
      RowBox[{"(", 
       RowBox[{"2", "l"}], ")"}]}], 
     RowBox[{"(", "m", ")"}]}], " ", "=", " ", 
    RowBox[{
     RowBox[{
      SubscriptBox[
       RowBox[{"(", 
        SubscriptBox["M", "0"], ")"}], 
       RowBox[{"\[Mu]", ",", "\[Nu]"}]], 
      RowBox[{
       SubscriptBox["N", "\[Nu]"], "^", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "l"}], "-", "1"}], ")"}]}], 
      RowBox[{"(", 
       RowBox[{"-", "m"}], ")"}]}], " ", "+", " ", 
     RowBox[{
      SubscriptBox[
       RowBox[{"(", 
        SubscriptBox["M", "1"], ")"}], 
       RowBox[{"\[Mu]", ",", "\[Nu]"}]], 
      RowBox[{
       SubscriptBox["N", "\[Nu]"], "^", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "l"}], "-", "1"}], ")"}]}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", "m"}], "-", "1"}], ")"}]}]}]}], TraditionalForm]]],
 "\nand thus:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["N", "\[Mu]"], "^", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "l"}], "-", "1"}], ")"}]}], 
     RowBox[{"(", 
      RowBox[{"-", "m"}], ")"}]}], "         ", "=", " ", 
    RowBox[{
     RowBox[{
      SubscriptBox[
       RowBox[{"(", 
        SubscriptBox["M", "0"], ")"}], 
       RowBox[{"\[Mu]", ",", "\[Nu]"}]], 
      RowBox[{
       SubscriptBox["N", "\[Nu]"], "^", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "l"}], "-", "2"}], ")"}]}], 
      RowBox[{"(", "m", ")"}]}], " ", "+", " ", 
     RowBox[{
      SubscriptBox[
       RowBox[{"(", 
        SubscriptBox["M", "1"], ")"}], 
       RowBox[{"\[Mu]", ",", "\[Nu]"}]], 
      RowBox[{
       SubscriptBox["N", "\[Nu]"], "^", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "l"}], "-", "2"}], ")"}]}], 
      RowBox[{"(", 
       RowBox[{"m", "-", "1"}], ")"}], " "}]}]}], TraditionalForm]]],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["N", "\[Mu]"], "^", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "l"}], "-", "1"}], ")"}]}], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", "m"}], "-", "1"}], ")"}]}], " ", "=", " ", 
    RowBox[{
     RowBox[{
      SubscriptBox[
       RowBox[{"(", 
        SubscriptBox["M", "0"], ")"}], 
       RowBox[{"\[Mu]", ",", "\[Nu]"}]], 
      RowBox[{
       SubscriptBox["N", "\[Nu]"], "^", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "l"}], "-", "2"}], ")"}]}], 
      RowBox[{"(", 
       RowBox[{"m", "+", "1"}], ")"}]}], " ", "+", " ", 
     RowBox[{
      SubscriptBox[
       RowBox[{"(", 
        SubscriptBox["M", "1"], ")"}], 
       RowBox[{"\[Mu]", ",", "\[Nu]"}]], 
      RowBox[{
       SubscriptBox["N", "\[Nu]"], "^", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "l"}], "-", "2"}], ")"}]}], 
      RowBox[{"(", "m", ")"}], " "}]}]}], TraditionalForm]]],
 "\nSo that,\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["N", "\[Mu]"], "^", 
      RowBox[{"(", 
       RowBox[{"2", "l"}], ")"}]}], 
     RowBox[{"(", "m", ")"}]}], " ", "=", " ", 
    RowBox[{
     RowBox[{
      SubscriptBox[
       RowBox[{"(", 
        SubscriptBox["M", "0"], ")"}], 
       RowBox[{"\[Mu]", ",", "\[Nu]"}]], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         SubscriptBox[
          RowBox[{"(", 
           SubscriptBox["M", "0"], ")"}], 
          RowBox[{"\[Nu]", ",", "\[Sigma]"}]], 
         RowBox[{
          SubscriptBox["N", "\[Sigma]"], "^", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"2", "l"}], "-", "2"}], ")"}]}], 
         RowBox[{"(", "m", ")"}]}], " ", "+", " ", 
        RowBox[{
         SubscriptBox[
          RowBox[{"(", 
           SubscriptBox["M", "1"], ")"}], 
          RowBox[{"\[Nu]", ",", "\[Sigma]"}]], 
         RowBox[{
          SubscriptBox["N", "\[Nu]"], "^", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"2", "l"}], "-", "2"}], ")"}]}], 
         RowBox[{"(", 
          RowBox[{"m", "-", "1"}], ")"}]}]}], " ", ")"}]}], " ", "+", " ", 
     RowBox[{
      SubscriptBox[
       RowBox[{"(", 
        SubscriptBox["M", "1"], ")"}], 
       RowBox[{"\[Mu]", ",", "\[Nu]"}]], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         SubscriptBox[
          RowBox[{"(", 
           SubscriptBox["M", "0"], ")"}], 
          RowBox[{"\[Nu]", ",", "\[Sigma]"}]], 
         RowBox[{
          SubscriptBox["N", "\[Sigma]"], "^", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"2", "l"}], "-", "2"}], ")"}]}], 
         RowBox[{"(", 
          RowBox[{"m", "+", "1"}], ")"}]}], " ", "+", " ", 
        RowBox[{
         SubscriptBox[
          RowBox[{"(", 
           SubscriptBox["M", "1"], ")"}], 
          RowBox[{"\[Nu]", ",", "\[Sigma]"}]], 
         RowBox[{
          SubscriptBox["N", "\[Sigma]"], "^", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"2", "l"}], "-", "2"}], ")"}]}], 
         RowBox[{"(", "m", ")"}]}]}], " ", ")"}]}]}]}], TraditionalForm]]],
 "\nand finally:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["N", "\[Mu]"], "^", 
      RowBox[{"(", 
       RowBox[{"2", "l"}], ")"}]}], 
     RowBox[{"(", "m", ")"}]}], " ", "=", " ", 
    RowBox[{
     RowBox[{
      SubscriptBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["M", "0"], "^", "2"}], "+", 
         RowBox[{
          SubscriptBox["M", "1"], "^", "2"}]}], ")"}], 
       RowBox[{"\[Mu]", ",", "\[Sigma]"}]], 
      RowBox[{
       SubscriptBox["N", "\[Sigma]"], "^", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "l"}], "-", "2"}], ")"}]}], 
      RowBox[{"(", "m", ")"}]}], "+", 
     RowBox[{
      SubscriptBox[
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["M", "0"], 
         SubscriptBox["M", "1"]}], ")"}], 
       RowBox[{"\[Mu]", ",", "\[Sigma]"}]], 
      RowBox[{
       SubscriptBox["N", "\[Sigma]"], "^", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "l"}], "-", "2"}], ")"}]}], 
      RowBox[{"(", 
       RowBox[{"m", "-", "1"}], ")"}]}], "+", 
     RowBox[{
      SubscriptBox[
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["M", "1"], 
         SubscriptBox["M", "0"]}], ")"}], 
       RowBox[{"\[Mu]", ",", "\[Sigma]"}]], 
      RowBox[{
       SubscriptBox["N", "\[Sigma]"], "^", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "l"}], "-", "2"}], ")"}]}], 
      RowBox[{"(", 
       RowBox[{"m", "+", "1"}], ")"}]}]}]}], TraditionalForm]]],
 "\nNow, summing over m, we obtain a recursion relation for ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["N", "~"], TraditionalForm]]],
 ":\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\n", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox[
        OverscriptBox["N", "~"], "\[Mu]"], "^", 
       RowBox[{"(", 
        RowBox[{"2", "l"}], ")"}]}], 
      RowBox[{"(", "t", ")"}]}], " ", "=", " ", 
     RowBox[{
      RowBox[{
       SubscriptBox["T", 
        RowBox[{"\[Mu]", ",", "\[Vee]"}]], "(", "t", ")"}], 
      RowBox[{
       SubscriptBox[
        OverscriptBox["N", "~"], "\[Nu]"], "^", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "l"}], "-", "2"}], ")"}]}], 
      RowBox[{"(", "t", ")"}]}]}]}], TraditionalForm]]],
 "\nwhere T is a sort of generalization of the inflation matrix squared:\n\
T(t) = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["M", "0"], "^", "2"}], "+", 
    RowBox[{
     SubscriptBox["M", "1"], "^", "2"}]}], TraditionalForm]]],
 " + t ",
