Cell["\<\
Let us check the numerical sum agrees with the result obtained by Gaussian \
integration.\
\>", "Text",
 CellChangeTimes->{{3.671883908180867*^9, 3.6718839391567917`*^9}, {
  3.671887308497129*^9, 3.671887337953188*^9}},
 Background->GrayLevel[0.85]],

Cell[BoxData[
 RowBox[{
  RowBox[{"NumSum", "[", 
   RowBox[{"avm_", ",", "t_", ",", "z_"}], "]"}], ":=", 
  RowBox[{"Sum", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"z", "^", 
      RowBox[{"(", "m", ")"}]}], " ", 
     RowBox[{"P", "[", 
      RowBox[{
       RowBox[{"m", "-", "avm"}], ",", "t"}], "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"m", ",", 
      RowBox[{
       RowBox[{"-", "t"}], "/", "2"}], ",", 
      RowBox[{"t", "/", "2"}]}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.6718839534395247`*^9, 3.671883989485138*^9}, {
  3.671884214437602*^9, 3.671884218149535*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Block", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"z", "=", "2."}], ",", 
     RowBox[{"avm", "=", "1.5"}], ",", "nums", ",", "tTable", ",", "th"}], 
    "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"tTable", "=", 
     RowBox[{"Range", "[", 
      RowBox[{"0.1", ",", 
       RowBox[{"10", "^", "2"}], ",", "1"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"nums", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Log", "[", 
         RowBox[{"NumSum", "[", 
          RowBox[{"avm", ",", "t", ",", "z"}], "]"}], "]"}], "-", 
        RowBox[{"t", " ", "d", " ", 
         RowBox[{
          RowBox[{"Log", "[", "z", "]"}], "^", "2"}]}]}], ",", 
       RowBox[{"{", 
        RowBox[{"t", ",", "tTable"}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"th", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"Log", "[", 
        RowBox[{"z", "^", "avm"}], " ", "]"}], "  ", ",", 
       RowBox[{"{", 
        RowBox[{"t", ",", "tTable"}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"ListPlot", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"glue", "[", 
        RowBox[{"tTable", ",", "#"}], "]"}], "&"}], "/@", 
      RowBox[{"{", 
       RowBox[{"nums", ",", "th"}], "}"}]}], "]"}]}]}], "\[IndentingNewLine]",
